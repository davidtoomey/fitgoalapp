<p id="notice"><%= notice %></p>

<h1>Listing Daily Caloric Intakes</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Calories</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @daily_caloric_intakes.each do |daily_caloric_intake| %>
      <tr>
        <td><%= daily_caloric_intake.user_id %></td>
        <td><%= daily_caloric_intake.calories %></td>
        <td><%= link_to 'Show', daily_caloric_intake %></td>
        <td><%= link_to 'Edit', edit_daily_caloric_intake_path(daily_caloric_intake) %></td>
        <td><%= link_to 'Destroy', daily_caloric_intake, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Daily caloric intake', new_daily_caloric_intake_path %>
